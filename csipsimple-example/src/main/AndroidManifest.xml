<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.project.csipsimple_example">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name=".ApiResponseReceiver"
            android:exported="true">
            <intent-filter>
                <action android:name="csipsimple.broadcast.action.api_response" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".UpdateRegistrationActivity"
            android:label="@string/title_activity_update_registration"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
